<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Cscope wx3.0.x" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/CodeBlocks/plugins/Cscope" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/Cscope" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-check-associations -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="../../../devel30" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel30\share\CodeBlocks\Cscope.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/Cscope/CscopeConfig.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeConfig.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeEntryData.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeEntryData.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeParserThread.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeParserThread.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopePlugin.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopePlugin.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeProcess.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeProcess.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeStatusMessage.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeStatusMessage.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeTab.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeTab.h" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeView.cpp" />
		<Unit filename="../src/plugins/contrib/Cscope/CscopeView.h" />
		<Unit filename="../src/plugins/contrib/Cscope/manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
