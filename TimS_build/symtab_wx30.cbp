<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="SymTab wx3.0.x" />
		<Option pch_mode="0" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(CB_BIN)/share/CodeBlocks/plugins/SymTab.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objs30/plugins/contrib/symtab" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="codeblocks" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="../../../devel30" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel30\share\codeblocks\SymTab.zip resources\manifest.xml resources\*.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/symtab/resources/config_dialog.xrc" />
		<Unit filename="../src/plugins/contrib/symtab/resources/exec_dialog.xrc" />
		<Unit filename="../src/plugins/contrib/symtab/resources/manifest.xml" />
		<Unit filename="../src/plugins/contrib/symtab/symtab.cpp" />
		<Unit filename="../src/plugins/contrib/symtab/symtab.h" />
		<Unit filename="../src/plugins/contrib/symtab/symtabconfig.cpp" />
		<Unit filename="../src/plugins/contrib/symtab/symtabconfig.h" />
		<Unit filename="../src/plugins/contrib/symtab/symtabexec.cpp" />
		<Unit filename="../src/plugins/contrib/symtab/symtabexec.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
