<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Games wx3.0.x" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/CodeBlocks/plugins/byogames.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/byogames" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/tinyxml" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="../../../devel30" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel30\share\CodeBlocks\byogames.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/byogames/byocbtris.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byocbtris.h" />
		<Unit filename="../src/plugins/contrib/byogames/byoconf.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byoconf.h" />
		<Unit filename="../src/plugins/contrib/byogames/byogame.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byogame.h" />
		<Unit filename="../src/plugins/contrib/byogames/byogamebase.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byogamebase.h" />
		<Unit filename="../src/plugins/contrib/byogames/byogames.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byogames.h" />
		<Unit filename="../src/plugins/contrib/byogames/byogameselect.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byogameselect.h" />
		<Unit filename="../src/plugins/contrib/byogames/byosnake.cpp" />
		<Unit filename="../src/plugins/contrib/byogames/byosnake.h" />
		<Unit filename="../src/plugins/contrib/byogames/manifest.xml" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/byoGameSelect.wxs" src="byogameselect.cpp" hdr="byogameselect.h" name="byoGameSelect" language="CPP" />
					<wxPanel wxs="wxsmith/byoConf.wxs" src="byoconf.cpp" hdr="byoconf.h" name="byoConf" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
