<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EnvVars wx3.0.x" />
		<Option pch_mode="0" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(CB_BIN)/share/codeblocks/plugins/envvars.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objs30/plugins/contrib/envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-DNOPCH" />
				</Compiler>
			</Target>
			<Target title="no_pch">
				<Option output="../../../devel30/share/codeblocks/plugins/envvars.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-DNOPCH" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="CB_WORKDIR" value="..\src\plugins\contrib\envvars" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(CB_INCLUDE)" />
			<Add directory="$(CB_INCLUDE)/scripting/include" />
			<Add directory="$(CB_INCLUDE)/scripting/sqplus" />
			<Add directory="$(CB_INCLUDE)/scripting/bindings" />
			<Add directory="$(CB_BASE)/sdk/wxscintilla/include" />
			<Add directory="$(#WX30.INCLUDE)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(CB_BIN)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="CMD /C cd $(CB_WORKDIR) &amp; zip -j9 $(CB_BIN)\share\codeblocks\envvars.zip manifest.xml *.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/envvars/envvars.cpp" />
		<Unit filename="../src/plugins/contrib/envvars/envvars.h" />
		<Unit filename="../src/plugins/contrib/envvars/envvars.xrc" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_cfgdlg.cpp" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_cfgdlg.h" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_common.cpp" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_common.h" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_prjoptdlg.cpp" />
		<Unit filename="../src/plugins/contrib/envvars/envvars_prjoptdlg.h" />
		<Unit filename="../src/plugins/contrib/envvars/manifest.xml" />
		<Unit filename="../src/plugins/contrib/envvars/project_settings.xrc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
