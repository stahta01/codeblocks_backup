<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Library finder wx3.0.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/CodeBlocks/plugins/lib_finder.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/lib_finder" />
				<Option external_deps="../../../devel30/wxflatnotebook.dll;" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectResourceIncludeDirsRelation="0" />
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../include/scripting/include" />
			<Add directory="../../../include/scripting/sqplus" />
			<Add directory="../../../include/scripting/squirrel" />
			<Add directory="../../../include/scripting/bindings" />
			<Add directory="../wxContribItems/wxFlatNotebook/include" />
			<Add directory="../../../include/mozilla_chardet" />
			<Add directory="../../../include/mozilla_chardet/mfbt" />
			<Add directory="../../../include/mozilla_chardet/nsprpub/pr/include" />
			<Add directory="../../../include/mozilla_chardet/xpcom" />
			<Add directory="../../../include/mozilla_chardet/xpcom/base" />
			<Add directory="../../../include/mozilla_chardet/xpcom/glue" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxflatnotebook" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="../../../devel30" />
		</Linker>
		<ExtraCommands>
			<Add after="update30.bat" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/lib_finder/defsdownloaddlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/defsdownloaddlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/dirlistdlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/dirlistdlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/headersdetectordlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/headersdetectordlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder/codeblocks_sdk.xml" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder/glfw.xml" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder/readme.txt" />
		<Unit filename="../src/plugins/contrib/lib_finder/lib_finder/wxwidgets.xml" />
		<Unit filename="../src/plugins/contrib/lib_finder/librariesdlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/librariesdlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/librarydetectionconfig.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/librarydetectionmanager.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/librarydetectionmanager.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/libraryresult.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/libraryresult.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/libselectdlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/libselectdlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/manifest.xml" />
		<Unit filename="../src/plugins/contrib/lib_finder/pkgconfigmanager.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/pkgconfigmanager.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/processingdlg.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/processingdlg.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectconfiguration.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectconfiguration.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectconfigurationpanel.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectconfigurationpanel.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectmissinglibs.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/projectmissinglibs.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/resultmap.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/resultmap.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/webresourcesmanager.cpp" />
		<Unit filename="../src/plugins/contrib/lib_finder/webresourcesmanager.h" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/DefsDownloadDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/DirListDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/HeadersDetectorDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/LibSelectDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/LibrariesDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/ProcessingDlg.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/ProjectConfigurationPanel.wxs" />
		<Unit filename="../src/plugins/contrib/lib_finder/wxsmith/ProjectMissingLibs.wxs" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/DirListDlg.wxs" src="dirlistdlg.cpp" hdr="dirlistdlg.h" fwddecl="1" name="DirListDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/ProcessingDlg.wxs" src="processingdlg.cpp" hdr="processingdlg.h" fwddecl="1" name="ProcessingDlg" language="CPP" />
					<wxPanel wxs="wxsmith/ProjectConfigurationPanel.wxs" src="projectconfigurationpanel.cpp" hdr="projectconfigurationpanel.h" fwddecl="1" name="ProjectConfigurationPanel" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/LibSelectDlg.wxs" src="libselectdlg.cpp" hdr="libselectdlg.h" fwddecl="1" name="LibSelectDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/LibrariesDlg.wxs" src="librariesdlg.cpp" hdr="librariesdlg.h" fwddecl="1" name="LibrariesDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/HeadersDetectorDlg.wxs" src="headersdetectordlg.cpp" hdr="headersdetectordlg.h" fwddecl="1" name="HeadersDetectorDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/DefsDownloadDlg.wxs" src="defsdownloaddlg.cpp" hdr="defsdownloaddlg.h" fwddecl="1" name="DefsDownloadDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/ProjectMissingLibs.wxs" src="projectmissinglibs.cpp" hdr="projectmissinglibs.h" fwddecl="1" name="ProjectMissingLibs" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
