<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Code::Blocks Share Config wx3.0.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(CB_BIN)/cb_share_config" prefix_auto="0" extension_auto="1" />
				<Option working_dir="$(CB_BIN)" />
				<Option object_output=".objs30/tools/cb_share_config" />
				<Option type="0" />
				<Option compiler="gcc" />
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DUSE_PCH" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWXUSINGDLL" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="$(CB_INCLUDE)" />
			<Add directory="$(CB_INCLUDE)/tinyxml" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="txml30" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="$(CB_LIB)/base/tinyxml" />
		</Linker>
		<Unit filename="../src/tools/cb_share_config/app.cpp" />
		<Unit filename="../src/tools/cb_share_config/app.h" />
		<Unit filename="../src/tools/cb_share_config/mainframe.cpp" />
		<Unit filename="../src/tools/cb_share_config/mainframe.h" />
		<Unit filename="../src/tools/cb_share_config/wxsmith/MainFrame.wxs" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="app.cpp" main="MainFrame" init_handlers="necessary" language="CPP" />
				<resources>
					<wxFrame wxs="wxsmith/MainFrame.wxs" src="mainframe.cpp" hdr="mainframe.h" name="MainFrame" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
