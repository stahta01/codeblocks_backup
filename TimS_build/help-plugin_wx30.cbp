<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help wx3.0.x" />
		<Option pch_mode="0" />
		<Option default_target="help_plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="bzip2">
				<Option output="$(CB_LIB)/bzip2/libbzip2.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output=".objs30/plugins/contrib/help_plugin/bzip2" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="zlib">
				<Option output="$(CB_LIB)/zlib/libz.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output=".objs30/plugins/contrib/help_plugin/zlib" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="help_plugin">
				<Option output="$(CB_BIN)/share/CodeBlocks/plugins/help_plugin.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objs30/plugins/contrib/help_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DNOPCH" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(CB_WORKDIR)/bzip2" />
					<Add directory="$(CB_WORKDIR)/zlib" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="bzip2" />
					<Add library="z" />
					<Add directory="$(CB_LIB)/bzip2" />
					<Add directory="$(CB_LIB)/zlib" />
				</Linker>
				<ExtraCommands>
					<Add after="CMD /C cd $(CB_WORKDIR) &amp; zip -j9 $(CB_BIN)\share\CodeBlocks\help_plugin.zip manifest.xml *.xrc *.png" />
					<Add after="cmd /c if not exist $(CB_BIN)\share\CodeBlocks\docs mkdir $(CB_BIN)\share\CodeBlocks\docs" />
					<Add after="xcopy /Y $(CB_WORKDIR)\index.ini $(CB_BIN)\share\CodeBlocks\docs" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="CB_WORKDIR" value="..\src\plugins\contrib\help_plugin" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="bzip2;zlib;help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="$(CB_INCLUDE)" />
			<Add directory="$(CB_INCLUDE)/scripting/bindings" />
			<Add directory="$(CB_INCLUDE)/scripting/include" />
			<Add directory="$(CB_INCLUDE)/scripting/sqplus" />
			<Add directory="$(CB_BASE)/sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add option="-Wl,--enable-auto-import" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(CB_BIN)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
