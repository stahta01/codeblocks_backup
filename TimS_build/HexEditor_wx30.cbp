<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="HexEditor wx3.0.x" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(CB_BIN)/share/CodeBlocks/plugins/HexEditor" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs30/plugins/contrib/HexEditor" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DcbDEBUG" />
					<Add option="-DNOPCH" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(#WX30.include)" />
					<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
					<Add directory="../../../include" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,--enable-auto-import" />
					<Add option="-mthreads" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add library="codeblocks" />
					<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
					<Add directory="../../../devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel30\share\CodeBlocks\HexEditor.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="../src/plugins/contrib/HexEditor/CharacterView.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/CharacterView.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/DigitView.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/DigitView.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionExecutor.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionExecutor.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionParser.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionParser.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionPreprocessed.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionPreprocessed.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionTestCases.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionTestCases.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionTester.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/ExpressionTester.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentBase.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentBase.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentBuffered.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentBuffered.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentDisk.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/FileContentDisk.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditLineBuffer.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditLineBuffer.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditPanel.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditPanel.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditViewBase.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditViewBase.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditor.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/HexEditor.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/Makefile.am" />
		<Unit filename="../src/plugins/contrib/HexEditor/SearchDialog.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/SearchDialog.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/SelectStoredExpressionDlg.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/SelectStoredExpressionDlg.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/TestCasesBase.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/TestCasesDlg.cpp" />
		<Unit filename="../src/plugins/contrib/HexEditor/TestCasesDlg.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/TestCasesHelper.h" />
		<Unit filename="../src/plugins/contrib/HexEditor/manifest.xml" />
		<Unit filename="../src/plugins/contrib/HexEditor/wxsmith/ExpressionTester.wxs" />
		<Unit filename="../src/plugins/contrib/HexEditor/wxsmith/HexEditPanel.wxs" />
		<Unit filename="../src/plugins/contrib/HexEditor/wxsmith/SearchDialog.wxs" />
		<Unit filename="../src/plugins/contrib/HexEditor/wxsmith/SelectStoredExpressionDlg.wxs" />
		<Unit filename="../src/plugins/contrib/HexEditor/wxsmith/TestCasesDlg.wxs" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/HexEditPanel.wxs" src="HexEditPanel.cpp" hdr="HexEditPanel.h" name="HexEditPanel" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/ExpressionTester.wxs" src="ExpressionTester.cpp" hdr="ExpressionTester.h" name="ExpressionTester" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/SelectStoredExpressionDlg.wxs" src="SelectStoredExpressionDlg.cpp" hdr="SelectStoredExpressionDlg.h" name="SelectStoredExpressionDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/TestCasesDlg.wxs" src="TestCasesDlg.cpp" hdr="TestCasesDlg.h" name="TestCasesDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/SearchDialog.wxs" src="SearchDialog.cpp" hdr="SearchDialog.h" name="SearchDialog" language="CPP" />
				</resources>
			</wxsmith>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
