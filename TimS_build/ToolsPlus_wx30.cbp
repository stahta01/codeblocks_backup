<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Tools Plus Plugin wx3.0.x" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeblocks/plugins/ToolsPlus" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/ToolsPlus" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add library="gdi32" />
					<Add directory="$(CB_BIN)" />
					<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="CB_BASE" value="$(#CB)" />
				<Variable name="CB_BIN" value="$(#CB.bin)" />
				<Variable name="CB_INCLUDE" value="$(#CB.include)" />
				<Variable name="CB_LIB" value="$(#CB.lib)" />
				<Variable name="CB_WORKDIR" value="..\src\plugins\contrib\ToolsPlus" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(CB_INCLUDE)" />
			<Add directory="../../../include/mozilla_chardet" />
			<Add directory="../../../include/mozilla_chardet/mfbt" />
			<Add directory="../../../include/mozilla_chardet/nsprpub/pr/include" />
			<Add directory="../../../include/mozilla_chardet/xpcom" />
			<Add directory="../../../include/mozilla_chardet/xpcom/base" />
			<Add directory="../../../include/mozilla_chardet/xpcom/glue" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="$(CB_BASE)/sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<ExtraCommands>
			<Add after="CMD /C cd $(CB_WORKDIR) &amp;  zip -j9 $(CB_BIN)\share\codeblocks\ToolsPlus.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="../src/plugins/contrib/ToolsPlus/CmdConfigDialog.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/CmdConfigDialog.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/PipedProcessCtrl.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/PipedProcessCtrl.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/Resources/dialogs.fbp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/ShellCtrlBase.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/ShellCtrlBase.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/ToolsPlus.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/ToolsPlus.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/manifest.xml" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/se_globals.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/se_globals.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/shellproperties.cpp" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/shellproperties.h" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/update30" />
		<Unit filename="../src/plugins/contrib/ToolsPlus/update30.bat" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
