		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiNotebook/wxsAuiNotebook.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiNotebook/wxsAuiNotebook.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxSmithAuiToolBar.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxSmithAuiToolBar.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBar.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBar.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarItem.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarItem.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarItemBase.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarItemBase.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarLabel.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarLabel.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarSeparator.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarSeparator.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarSpacer.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiToolBar/wxsAuiToolBarSpacer.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxSmithAui.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxSmithAui.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
