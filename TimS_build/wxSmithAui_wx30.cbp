		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiManager/wxsAuiPaneInfoProperty.cpp" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiManager/wxsAuiPaneInfoProperty.h" />
		<Unit filename="../src/plugins/contrib/wxSmithAui/wxAuiNotebook/wxSmithAuiNotebook.cpp" />
