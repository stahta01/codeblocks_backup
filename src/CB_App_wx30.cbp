					<Add library="uuid" />
					<Add library="rpcrt4" />
					<Add library="advapi32" />
					<Add library="wsock32" />
					<Add directory="devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if not exist devel30\share\CodeBlocks mkdir devel30\share\CodeBlocks" />
					<Add after="zip -jq9 devel30\share\CodeBlocks\resources.zip src\resources\*.xrc" />
					<Add after="zip -jq9 devel30\share\CodeBlocks\start_here.zip src\resources\start_here\*.html src\resources\start_here\*.png" />
					<Add after='cmd /c &quot;cd src\resources &amp; zip -0 -q ..\..\devel30\share\CodeBlocks\resources.zip images\*.png images\16x16\*.png&quot;' />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="src;" />
			<Add alias="Core app" targets="src;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="sdk/wxscintilla/include" />
			<Add directory="include/tinyxml" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30)/lib/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</ResourceCompiler>
		<Linker>
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="base/tinyxml" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
